{
  "name": "Fair Fares",
  "description": "Best flight fares",
  "version": "1.0",
  "manifest_version": 2,
  "permissions": [
    "alarms",
    "notifications",
    "storage",
    "*://*.yatra.com/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "options_page": "options.html",
  "browser_action": {
    "default_title": "Fair Fares",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "drink_water16.png",
    "32": "drink_water32.png",
    "48": "drink_water48.png",
    "128": "drink_water128.png"
  }
}